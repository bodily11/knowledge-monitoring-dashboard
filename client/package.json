{
  "name": "wadayano-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tinymce/tinymce-react": "^3.0.1",
    "apollo-client-preset": "^1.0.8",
    "bulma": "^0.7.1",
    "butter-toast": "^3.1.0",
    "dom": "^0.0.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "immutability-helper": "^2.7.1",
    "json5": "^2.1.0",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.4.1",
    "react-aria-modal": "^4.0.0",
    "react-dom": "^16.7.0",
    "react-loadable": "^5.5.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.5",
    "react-scroll-into-view-if-needed": "^2.1.4",
    "react-show-more": "^2.0.0",
    "react-timeago": "^4.1.9",
    "react-tooltip": "^3.9.2",
    "react-tracking": "^5.7.0",
    "tinymce": "^5.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build  && rm -rf build-prod-old && mv build-prod build-prod-old && mv build build-prod",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
